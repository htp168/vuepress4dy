][style] with `.vuepress/styles/index.scss` file.\n\n[routing]: https://vuejs.press/guide/page.html#routing\n[content]: https://vuejs.press/guide/page.html#content\n[synatex-extensions]: https://vuejs.press/guide/markdown.html#syntax-extensions\n[vue-feature]: https://vuejs.press/guide/markdown.html#using-vue-in-markdown\n[config]: https://vuejs.press/guide/configuration.html#client-config-file\n[client-config]: https://vuejs.press/guide/configuration.html#client-config-file\n[frontmatter]: https://vuejs.press/guide/page.html#frontmatter\n[navbar]: https://vuejs.press/reference/default-theme/config.html#navbar\n[sidebar]: https://vuejs.press/reference/default-theme/config.html#sidebar\n[default-theme]: https://vuejs.press/reference/default-theme/\n[style]: https://vuejs.press/reference/default-theme/styles.html#style-file","excerpt":"","includedFiles":[],"references":{"ROUTING":{"title":"","href":"https://vuejs.press/guide/page.html#routing"},"CONTENT":{"title":"","href":"https://vuejs.press/guide/page.html#content"},"SYNATEX-EXTENSIONS":{"title":"","href":"https://vuejs.press/guide/markdown.html#syntax-extensions"},"VUE-FEATURE":{"title":"","href":"https://vuejs.press/guide/markdown.html#using-vue-in-markdown"},"CONFIG":{"title":"","href":"https://vuejs.press/guide/configuration.html#client-config-file"},"CLIENT-CONFIG":{"title":"","href":"https://vuejs.press/guide/configuration.html#client-config-file"},"FRONTMATTER":{"title":"","href":"https://vuejs.press/guide/page.html#frontmatter"},"NAVBAR":{"title":"","href":"https://vuejs.press/reference/default-theme/config.html#navbar"},"SIDEBAR":{"title":"","href":"https://vuejs.press/reference/default-theme/config.html#sidebar"},"DEFAULT-THEME":{"title":"","href":"https://vuejs.press/reference/default-theme/"},"STYLE":{"title":"","href":"https://vuejs.press/reference/default-theme/styles.html#style-file"}},"tasklists":0,"title":"","headers":[{"level":2,"title":"Pages","slug":"pages","link":"#pages","children":[]},{"level